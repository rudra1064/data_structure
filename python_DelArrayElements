#python program for deleting any number in array
n=int(input("Enter the Number of elements in array :"))
print("enter the Elements")
l=[]
i=0
for i in range(n):
    ele=int(input())
    l.append(ele)
print("Original array: ", l)

specified_index=int(input("Enter the value to be deleted :"))
l.remove(specified_index)

print(" New Array : ", l)
