#include<iostream>
using namespace std;

int main()
{
int a[10],i,n,*ptr,key;
cout<<"How many Elements :\n";
cin>>n;
cout<<"Enter the Elements :\n";
for(i=0;i<n;i++)
{
  cin>>a[i];
}
ptr=&a[0];
cout<<"Enter the key :\n";
cin>>key;
for (i=0;i<=n;i++){
if(*ptr==key){
	cout<<"\nElement present !:";
	break;
}
else if{
	ptr++;
}

}

return 0;
}
