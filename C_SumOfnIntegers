#include<stdio.h>
int main()
{
int i,n,sum=0;
printf("Enter the Last Number you want Sum To : ");
scanf("%d",&n);
//Time Complexeity More (Not Good Method)
for(i=1;i<=n;i++)
{
 sum+=i;
}
printf("The Sum Is : %d \n",sum);
//Less Time Complexity (Good Method)
int SumOfAll=(n*n+n)/2;
printf("The Sum is : %d",SumOfAll);

return 0;
}
