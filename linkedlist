#include<iostream>
using namespace std;
struct node{
    int data;
   struct node*next;
};
//Creating Node 
node*create(){
    node*p=new(struct node);
    int a;
    cout<<"Enter the Data \n:";
    cin>>a;
    p->data=a;
    p->next=NULL;
    return p;
}
//InsertionAtFront    
void insertatfront(){
    node*head;
  node*new_node=new node();
  create();
  if (head==NULL){
      head=new_node;
  }
  else{
     new_node->next=head; 
  }
  
 //Transverse The List
 void Traverse(){
node* temp = new node();
if(head==NULL)
{
cout<<"linklist is empty";
}
else
{
temp=head;
cout<<" data \n";
while(temp!=NULL)
{
cout<<"-->"<<temp->data;
temp=temp->next;
}
}
}

int main(){
    node*newNode=create();
    cout<<newNode->data;
    insertatfront();
}
