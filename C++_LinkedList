#include<iostream>
using namespace std;
struct node{
    int data;
   struct node*next;
};
node*create(){
    node*p=new(struct node);
    int a;
    cout<<"Enter the Data :";
    cin>>a;
    p->data=a;
    p->next=NULL;
    return p;
}
    
void insertatfront(){
  node*head;
  node*new_node=new node();
  int data;
  cout<<"\nEnter the Data";
  cin>>data;
  new_node->data=data;
  new_node->next=head;
  head=new_node;
  cout<<"Inserted Element :"<<new_node->data<<"->"<<endl;
}
int main(){
    node*newNode=create();
    cout<<newNode->data;
    insertatfront();
}
